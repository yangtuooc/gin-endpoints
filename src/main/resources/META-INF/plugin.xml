<idea-plugin package="cn.yangtuooc.gin.endpoints">
  <id>cn.yangtuooc.gin</id>

  <name>Gin Endpoints</name>
  <vendor>yangtuooc</vendor>

  <description><![CDATA[
      Gin Endpoints
  ]]></description>

  <dependencies>
    <plugin id="com.intellij.modules.go"/>
    <plugin id="com.intellij.modules.microservices"/>
    <plugin id="com.intellij.modules.ultimate"/>
  </dependencies>

  <extensions defaultExtensionNs="com.intellij">
    <psi.referenceContributor language="go" implementation="cn.yangtuooc.gin.endpoints.GinReferenceContributor"/>
    <microservices.endpointsProvider implementation="cn.yangtuooc.gin.endpoints.GinEndpointsProvider"/>
    <microservices.urlResolverFactory implementation="cn.yangtuooc.gin.endpoints.GinUrlResolverFactory"/>
    <dependencySupport kind="go" coordinate="github.com/gin-gonic/gin" displayName="Gin Framework"/>
    <implicitUsageProvider implementation="cn.yangtuooc.gin.endpoints.GinImplicitUsageProvider"/>
  </extensions>

</idea-plugin>